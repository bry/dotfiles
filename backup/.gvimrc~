"   macmenu &File.New\ Tab key=<nop>
"   map <D-/> ,c<space><CR>
" endif

" ConqueTerm wrapper
function StartTerm()
  execute 'ConqueTerm ' . $SHELL . ' --login'
  setlocal listchars=tab:\ \ 
endfunction

function CloseAllBuffers()
  let bufs = tabpagebuflist()
endfunction

" Command-e for ConqueTerm
map <Leader>e :call StartTerm()<CR>
